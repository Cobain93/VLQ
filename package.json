{
  "name": "vlq",
  "version": "0.0.1",
  "author": "cobain",
  "description": "vlq",
  "license": "MIT",
  "keywords": [
    "webpack",
    "typescript",
    "vlq",
    "source map"
  ],
  "main": "index.js",
  "scripts": {
    "watch": "npm run clean && webpack --watch --progress --env.umd",
    "clean": "rimraf ./dist",
    "upload": "node cs_upload.js",
    "serve": "http-server --cors -p 3001",
    "dev": "cross-env NODE_ENV=development cross-env umd=true webpack-dev-server",
    "build:cjs": "cross-env NODE_ENV=production cross-env cjs=true webpack",
    "build:umd": "cross-env NODE_ENV=production cross-env umd=true webpack",
    "build:amd": "cross-env NODE_ENV=production cross-env amd=true webpack",
    "build": "npm run clean && cross-env NODE_ENV=production && npm run build:cjs && npm run build:amd && npm run build:umd",
    "test": "mocha test/index.ts"
  },
  "devDependencies": {
    "cross-env": "^5.2.0",
    "html-webpack-plugin": "3.2.0",
    "http-server": "0.11.1",
    "rimraf": "2.6.2",
    "string-replace-loader": "2.1.1",
    "ts-loader": "^3.5.0",
    "typescript": "3.1.3",
    "webpack": "^1.15.0"
  },
  "dependencies": {
    "webpack-dev-server": "^1.16.5"
  }
}
